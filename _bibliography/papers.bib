	doi = {10.1002/macp.201700343},
	abstract = {Interchromophoric interactions between oligo(phenylene ethynylene)s incorporated in copolymers in solution as well as in solid state are investigated using UV/Vis and IR spectroscopy. One promising strategy to enable strong interchromophoric interactions is the introduction of hydrogen-bonding moieties. Together with the tendency of the chromophores to self-assemble via π–π-stacking a dense packing of the chromophores within a polymer film can be achieved. In order to demonstrate this strategy, oligo(phenylene ethynylene)s are used as monomers for copolymers using the highly efficient blocked isocyanate route and polymer films are prepared by spin coating technique. The copolymers are prepared by an in situ deprotection of a pyrazol protected bis-isocyanate functionalized rigid chromophore followed by subsequent polyaddition using a bis-amine or bis-alcohol linker unit. The specific introduction of urea as well as urethane moieties enhances coplanarization of the chromophores in the polymer films into well-organized structures, which can be characterized based on their specific optical properties.},
	language = {en},
	number = {22},
	urldate = {2021-11-08},
	journal = {Macromolecular Chemistry and Physics},
  abbr = {Macromol. Chem. Phys.},
	author = {Ahner, Johannes and Micheel, Mathias and Enke, Marcel and Zechel, Stefan and Schubert, Ulrich S. and Dietzek, Benjamin and Hager, Martin D.},
	year = {2017},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/macp.201700343},
	keywords = {conjugated polymers, hydrogen bonding, phenylene ethynylene, urea, urethane},
	pages = {1700343},
	file = {Ahner et al. - 2017 - Directed Orientation of Oligo(phenylene ethynylene.pdf:C\:\\Users\\Mathias\\Downloads\\Ahner et al. - 2017 - Directed Orientation of Oligo(phenylene ethynylene.pdf:application/pdf},
