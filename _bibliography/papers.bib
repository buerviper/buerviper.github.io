  data = {https://doi.org/10.5281/zenodo.7074260},
}

@article{micheel_assembly_2022,
	title = {Assembly, {Properties}, and {Application} of {Ordered} {Group} {II}–{VI} and {IV}–{VI} {Colloidal} {Semiconductor} {Nanoparticle} {Films}},
