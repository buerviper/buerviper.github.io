	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cctc.202400247},
	keywords = {FeFe-hydrogenase mimics, Hydrogen evolution, oligothiophene, Photocatalysis, scalar-relativist time-dependent density functional theory simulations},
	pages = {e202400247},
